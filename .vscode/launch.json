    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "server.py",
                "FLASK_ENV": "development"
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true
        },
        {
            "name": "fetch_municipality CLI (save to db)",
            "type": "python",
            "request": "launch",
            "program": "fetch_municipality.py",
            "args": [
                "--save_to_db"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "fetch_incidence CLI (save to db)",
            "type": "python",
            "request": "launch",
            "program": "fetch_incidence.py",
            "args": [
                "--save_to_db"
            ],
            "console": "integratedTerminal"
        }
    ]
}